import forcomp._

object w6 {
  println("Welcome to the Scala worksheet")       //> Welcome to the Scala worksheet
  Anagrams.combinations(List(('a',1),('b',2)))    //> res0: List[forcomp.Anagrams.Occurrences] = List(List(), List((a,1)), List((b
                                                  //| ,1)), List((b,2)), List((b,1), (a,1)), List((b,2), (a,1)))
}